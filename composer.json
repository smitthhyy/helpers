{
  "name": "smitthhyy/helpers",
  "type": "library",
  "description": "This is a collection of small functions used regularly in projects",
  "keywords": ["smitthhyy", "helpers", "library"],
  "license": "MIT",
  "authors": [
    {
      "name": "Trevor van der Linden"
    }
  ],
  "require": {
    "php": "^5.3.3 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.35 || ^5.7",
    "squizlabs/php_codesniffer": "^1.5",
    "ext-intl": "*"
  },
  "autoload": {
    "psr-4": {
      "Helpers\\": "src/Helpers/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Helpers\\Test\\": "test/Helpers/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.8-dev"
    }
  }
}